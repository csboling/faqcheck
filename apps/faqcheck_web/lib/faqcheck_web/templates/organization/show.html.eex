<h1><%= gettext "Show Organization" %></h1>

<ul>

  <li>
    <strong><%= gettext "Name" <> ":" %></strong>
    <%= @organization.name %>
  </li>

  <li>
    <strong><%= gettext "Description" <> ":" %></strong>
    <%= @organization.description %>
  </li>

</ul>

<span><%= link gettext("Edit"), to: Routes.organization_path(@conn, :edit, @locale, @organization) %></span>
<span><%= link gettext("Back"), to: Routes.organization_path(@conn, :index, @locale) %></span>
